{
    "name": "johannes85/ai-bundle",
    "description": "Adds AI support to Symfony. Allows easy calling of LLMs like Ollama, OpenAI, GoogleAI, MistralAI, Anthropic, DeepSeek. It also adds an MCP server and client.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AiBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Johannes Zinnau",
            "email": "johannes@johnimedia.de"
        }
    ],
    "require-dev": {
        "symfony/console": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/yaml": "^7.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11"
    },
    "require": {
        "PHP": ">=8.2",
        "symfony/serializer": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/http-client": "^7.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/property-access": "^7.0",
        "symfony/uid": "^7.0",
        "symfony/process": "^7.2"
    }
}
